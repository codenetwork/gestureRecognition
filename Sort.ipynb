   "source": [
    "from pathlib import Path\n",
    "import os\n",
    "\n",
    "p = Path(r\"path\")\n",
    "l = os.listdir(p)\n",
    "\n",
    "for position, file in enumerate(l):\n",
    "    old_path = p / file\n",
    "    new_path = p / f\"{position}{Path(file).suffix}\"\n",
    "    os.rename(old_path, new_path)\n"
   ]
  }
